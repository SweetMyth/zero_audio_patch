<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- Zero Audio Patch by vlad113 -->
<mixer>
	<path name="hph-amp-class">
		<ctl name="RX HPH Mode" value="CLS_H_HIFI"/>
	</path>

	<path name="hph-volume">
		<!-- Analog volume -->
		<ctl name="HPHL Volume" value="20"/>
		<ctl name="HPHR Volume" value="20"/>
		<!-- Digital volume -->
		<ctl name="RX1 Mix Digital Volume" value="94"/>
		<ctl name="RX2 Mix Digital Volume" value="94"/>
	</path>

	<path name="speaker-volume">
		<!-- do not set the value higher, you may damage the speaker. -->
		<ctl name="RX0 Digital Volume" value="94"/>
		<ctl name="RX7 Digital Volume" value="84"/>
	</path>

<!-- Output routes -->
	<path name="gsm-mode">
		<ctl name="GSM mode Enable" value="ON"/>
	</path>

	<path name="echo-reference">
		<ctl name="AUDIO_REF_EC_UL1 MUX" value="SLIM_RX"/>
	</path>

	<path name="echo-reference speaker-protected">
		<path name="echo-reference"/>
	</path>

	<path name="echo-reference speaker-vbat">
		<ctl name="AIF3_CAP Mixer SLIM TX1" value="1"/>
		<ctl name="SLIM TX1 MUX" value="RX_MIX_TX1"/>
		<ctl name="RX MIX TX1 MUX" value="RX_MIX_VBAT7"/>
		<ctl name="SLIM_1_TX Channels" value="One"/>
		<ctl name="VOC_EXT_EC MUX" value="SLIM_1_TX"/>
	</path>

	<path name="echo-reference headphones">
		<ctl name="AUDIO_REF_EC_UL1 MUX" value="SLIM_6_RX"/>
	</path>

	<path name="echo-reference headphones-44.1">
		<path name="echo-reference headphones"/>
	</path>

	<!-- Deep buffer route -->
	<path name="deep-buffer-playback">
		<ctl name="SLIMBUS_0_RX Audio Mixer MultiMedia1" value="1"/>
	</path>

	<path name="deep-buffer-playback speaker-protected">
